# Intel386 specific interface definition for usr/src/lib/libsys.


SYSVABI_1.2 {	# System V ABI as of the revised (2nd) edition 
	global:
		 _fxstat;		# Additional Required Entry Points
		 _lxstat;		# System V ABI (i386), Figure 6-1
		  nuname;
		 _nuname;
		 _xmknod;
		 _xstat;

		 _fpstart;		# Support Routines
		  sbrk;			# System V ABI (i386), Figure 6-2
		 _sbrk;

		__flt_rounds;		# Global Externl Data Symbols
		 _fp_hw;		# System V ABI (i386), Figure 6-3
		__huge_val;
};
