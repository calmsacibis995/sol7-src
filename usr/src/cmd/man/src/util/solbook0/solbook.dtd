        (((%chapter.gp;)+, Reference*) | (Part,Part+) | Reference+),
                  (%appendix.gp;)*, Glossary?, Bibliography?,(%index.gp;)* )
          +(%ubiq.gp;) >
<!ATTLIST Book
        %commonatts;
	Role	(SINGLE | NUMBERED | NUMBERED-NonConsecutivePg | UNNUMBERED |
		UNNUMBERED-NonConsecutivePg )  #IMPLIED
        FPI     CDATA       #IMPLIED
		Label (ALPHA | BETA | PRE-FCS | FCS)  #IMPLIED
>


